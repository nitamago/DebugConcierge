package TEST;

import org.eclipse.jdt.internal.compiler.batch.Main;

public class Test1 {

	public static void main(String[] args) {
		// Var Declare Test
		int i;
		int j, k;
		boolean l;
		boolean m, n;
		char c;
		char d, e;
		
		// Var assign Test
		i = 1;
		j = i;
		l = true;
		m = l;
		c = 'a';
		d = c;
		
		// Constructor Test
		Test1 main;
		main = new Test1();
		
		// Invoke Method Test
		main.say_hello();
		main.say_word("hoge");
		main.say_words("fuga", c+"");
	}
	
	public void say_hello(){
		System.out.println("Hello!");
	}
	
	public void say_word(String word){
		System.out.println(word);
	}
	
	public void say_words(String word1, String word2){
		System.out.println(word1+" "+word2);
	}

}
